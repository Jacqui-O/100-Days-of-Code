function FirstReverse(str) { 

  var str2 = str.split(""); //split the input string into an array
  var words = [];
  j=0;
  
    for(i=str.length-1; i>=0; i--){
        words[j] = str2[i]; 
        j++;
    }
  // code goes here  
  return words.join(""); 
  
}
   
// keep this function call here 
FirstReverse(readline());       
